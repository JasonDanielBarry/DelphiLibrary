unit DrawingAxisConversionClass;

interface

    uses
        System.SysUtils, system.Math,
        GeometryTypes;

    type
        TDrawingAxisConverter = class
            private
                //member variables
                    minPoint, maxPoint : TGeomPoint;
                //helper methods
                    //axis space boundaries
                        function domain() : double;
                        function range() : double;

        end;

implementation



end.
